{
  "name": "thepixeldeveloper/nolimits-exchange",
  "type": "project",
  "description": "Code for https://nolimits-exchange.com",
  "keywords": ["nolimits-exchange", "nolimits", "nolimits 2", "nolimits coaster 2"],
  "license": "MIT",
  "authors": [
    {
      "name": "Mathew Davies",
      "email": "thepixeldeveloper@googlemail.com",
      "homepage": "http://mathew-davies.co.uk",
      "role": "Developer"
    }
  ],
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.0.0",
    "symfony/symfony": "3.1.*",
    "doctrine/orm": "^2.5",
    "doctrine/doctrine-bundle": "^1.6",
    "doctrine/doctrine-cache-bundle": "^1.2",
    "symfony/swiftmailer-bundle": "^2.3",
    "symfony/monolog-bundle": "^2.8",
    "sensio/distribution-bundle": "^5.0",
    "sensio/framework-extra-bundle": "^3.0.2",
    "vlucas/phpdotenv": "^2.2",
    "friendsofsymfony/user-bundle": "dev-master",
    "stof/doctrine-extensions-bundle": "^1.2",
    "cocur/slugify": "^2.1",
    "doctrine/doctrine-fixtures-bundle": "^2.3",
    "doctrine/doctrine-migrations-bundle": "^1.1",
    "willdurand/faker-bundle": "^1.3",
    "knplabs/knp-markdown-bundle": "^1.5",
    "white-october/pagerfanta-bundle": "^1.0",
    "oneup/flysystem-bundle": "^1.4",
    "sentry/sentry-symfony": "^0.3.0",
    "phpunit/phpunit": "^4.8",
    "beberlei/DoctrineExtensions": "^1.0",
    "aws/aws-sdk-php": "^3.18",
    "league/flysystem-aws-s3-v3": "^1.0",
    "league/flysystem-memory": "^1.0",
    "intervention/image": "^2.3",
    "twig/extensions": "^1.3",
    "heristop/jobqueue-bundle": "dev-master",
    "thepixeldeveloper/nolimits2packageloader": "^1.1"
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^3.0",
    "phpmd/phpmd": "^2.4",
    "squizlabs/php_codesniffer": "^2.6",
    "brianium/paratest": "^0.13.3"
  },
  "autoload": {
    "psr-4": { "Thepixeldeveloper\\Nolimitsexchange\\": "src/" },
    "classmap": [ "app/AppKernel.php", "app/AppCache.php" ]
  },
  "autoload-dev": {
    "psr-4": { "Tests\\": "tests/" }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0.7"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
    ],
    "post-update-cmd": [
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
    ]
  },
  "extra": {
    "symfony-app-dir": "app",
    "symfony-web-dir": "html",
    "symfony-var-dir": "var",
    "symfony-bin-dir": "bin",
    "symfony-tests-dir": "tests",
    "symfony-assets-install": "relative",
    "branch-alias": {
      "dev-master": "3.2-dev"
    }
  }
}
