parameters:

    locale: "en"

    secret: "%env(APP_SECRET)%"

    mailer_transport: "smtp"
    mailer_host: "%env(EMAIL_HOST)%"
    mailer_port: "%env(EMAIL_PORT)%"
    mailer_user: "%env(EMAIL_USER)%"
    mailer_pass: "%env(EMAIL_PASS)%"

    database_driver: "pdo_mysql"
    database_host: "%env(MYSQL_HOST)%"
    database_port: "%env(MYSQL_PORT)%"
    database_user: "%env(MYSQL_USER)%"
    database_pass: "%env(MYSQL_PASSWORD)%"
    database_name: "%env(MYSQL_DATABASE)%"

    object_storage_bucket: "%env(OBJECT_STORAGE_BUCKET)%"
    object_storage_host: "%env(OBJECT_STORAGE_HOST)%"
    object_storage_key: "%env(OBJECT_STORAGE_KEY)%"
    object_storage_region: "%env(OBJECT_STORAGE_REGION)%"
    object_storage_secret: "%env(OBJECT_STORAGE_SECRET)%"
    object_storage_server: "%env(OBJECT_STORAGE_SERVER)%"
